 * 
 */

/**
 * @author RV
 *
 */
public class Q001 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		long limit;
		if(args.length == 0){			
			limit = 1000;
		}else{
			 limit = Long.valueOf(args[0]);
		}		
		long i=3,sum =0;
		while(i < limit){			
			if(i%3 == 0 || i%5 == 0){
				sum +=i;
			}
			i++;
		}
		System.out.println(sum);
	}

}
